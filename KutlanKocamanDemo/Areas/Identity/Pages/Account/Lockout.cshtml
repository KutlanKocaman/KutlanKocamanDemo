@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<header>
    <h1 class="text-danger">@ViewData["Title"]</h1>
    <p class="text-danger">This account has been locked out, please use the <a id="confirm-email" asp-page="./ForgotPassword">Forgot Password</a> page to unlock your account.</p>
</header>
